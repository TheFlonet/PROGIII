<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.2-internal"
      fx:controller="progiii.server.controller.EventController">
    <Label fx:id="details" minWidth="500.0" text="Label" VBox.vgrow="NEVER">
        <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </VBox.margin>
    </Label>
    <ListView fx:id="actionListView" focusTraversable="false" minHeight="300.0" minWidth="500.0" prefHeight="300.0"
              VBox.vgrow="ALWAYS"/>
</VBox>
