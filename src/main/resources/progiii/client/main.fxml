        </SplitPane>
    </center>
    <bottom>
        <AnchorPane BorderPane.alignment="CENTER">
            <Label fx:id="status" alignment="TOP_RIGHT" text="StatusBar" textAlignment="RIGHT" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="0.0">
                <padding>
                    <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                </padding>
            </Label>
        </AnchorPane>
    </bottom>
</BorderPane>
